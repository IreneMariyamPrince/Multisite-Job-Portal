{
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "endOfLine": "lf",
    "quoteProps": "as-needed",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 200
        }
      },
      {
        "files": "*.html",
        "options": {
          "printWidth": 200
        }
      },
      {
        "files": "*.css",
        "options": {
          "singleQuote": false,
          "trailingComma": "none"
        }
      }
    ]
  }
  